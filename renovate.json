{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "constraints": {
    "python": "==3.10"
  },
  "automerge": false,
  "baseBranches": ["develop"],
  "pip-compile": {
    "fileMatch": ["requirements.in", "requirements_dev.in"]
  },
  "lockFileMaintenance": {
    "enabled": true,
    "branchTopic": "pip-compile-refresh",
    "commitMessageAction": "Refresh pip-compile outputs"
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "gdal",
        "django-bootstrap-datepicker-plus",
        "django-bootstrap4",
        "django-two-factor-auth",
        "django-crispy-forms"
      ],
      "enabled": false
    }
  ]
}
