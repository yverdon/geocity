{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "docker:disable"
  ],
  "automerge": false,
  "baseBranches": ["develop"],
  "pip-compile": {
    "fileMatch": ["(^|/)requirements\\.in$"]
  },
  "ignoreDeps": ["gdal", "jango-bootstrap-datepicker-plus", "django-bootstrap4", "django-two-factor-auth[phonenumbers]"],
  "lockFileMaintenance": {
    "enabled": true,
    "branchTopic": "pip-compile-refresh",
    "commitMessageAction": "Refresh pip-compile outputs"
  }
}
