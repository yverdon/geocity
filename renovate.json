{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "docker:disable"
  ],
  "automerge": false,
  "baseBranches": ["develop"],
  "pip-compile": {
    "fileMatch": ["requirements.in", "requirements_dev.in"]
  },
  "pip_requirements": {
    "enabled": false
  },
  "lockFileMaintenance": {
    "enabled": true,
    "branchTopic": "pip-compile-refresh",
    "commitMessageAction": "Refresh pip-compile outputs"
  },
  "packageRules": [
    {
      "matchPackagePatterns": ["gdal", "django-bootstrap-datepicker-plus", "django-bootstrap4", "django-two-factor-auth[phonenumbers]", "django-crispy-forms"],
      "matchManagers": ["pip-compile"],
      "enabled": false
    }
  ]
}
